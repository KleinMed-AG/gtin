{
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/trigger",
      "destination": "/api/trigger.js"
    },
    {
      "source": "/api/download",
      "destination": "/api/download.js"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
